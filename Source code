let Discord = require("discord.js");
let client = new Discord.Client();

client.on("message", message => {
  if (message.content === "info SCP-096") {
    let embed = new Discord.MessageEmbed()
      .setTitle("SCP-096")
      .setColor("YELLOW")
      .setFooter(
        "SCP 096 is a powerful creature. If you look at him body, face, etc he will enrage the enrage phase is a dangerous phase if he doesnt get bagged there he can kill SCP-096-1 and even if he just looked at the picture at the other side of the earth!"
      );
    message.channel.send(embed);
  }
});
